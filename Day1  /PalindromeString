class PalindromeString {
    boolean isPalindrome(String s) {
        char[] ch=s.toCharArray();
        String s1="";
        for(int i=s.length()-1;i>=0;i--){
            s1+=ch[i];
        }
        if(s.equals(s1)){
            return true;
        }else{
            return false;
        }
        
    }
}
